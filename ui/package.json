		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-basic-ssl": "^1.0.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pdfjs-dist": "^4.0.269",
		"prettier": "^3.0.0",
