{
	"name": "insight-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"download-icons": "curl 'https://css.gg/css?=|comment|box|file|file-document|menu|search|log-out|folder|software-upload|software-download|loadbar|trash' -o ./src/lib/icons.css"
	},
	"devDependencies": {
		"@fontsource-variable/roboto-slab": "^5.0.18",
		"@fontsource-variable/rubik": "^5.0.20",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.2",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"@vitejs/plugin-basic-ssl": "^1.1.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"fast-xml-parser": "^4.3.5",
		"minio": "^7.1.3",
		"pdfjs-dist": "^4.0.379",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"sveltekit-search-params": "^2.1.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.4"
	},
