{
	"name": "insight-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port=3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"download-icons": "curl 'https://css.gg/css?=|box|comment|copy|more-vertical-alt|file|file-document|menu|search|log-out|folder|folder-add|software-upload|software-download|loadbar|add|trash|pen|check-o|eye|chevron-right|chevron-double-right|chevron-left|chevron-double-left|home' -o ./src/lib/icons.css"
	},
	"devDependencies": {
		"@fontsource-variable/roboto-slab": "^5.0.20",
		"@fontsource-variable/rubik": "^5.0.22",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.5.18",
		"@types/amqplib": "^0.10.5",
		"@types/node": "^20.14.11",
		"@types/pdfjs-dist": "^2.10.377",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"fast-xml-parser": "^4.4.0",
		"minio": "^8.0.1",
		"pdfjs-dist": "^4.4.168",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"svelte-multiselect": "^10.3.0",
		"sveltekit-search-params": "^2.1.2",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^5.3.4"
	},
	"type": "module",
	"dependencies": {
		"amqplib": "^0.10.4",
		"rollup": "^4.19.0",
		"zod": "^3.23.8"
	}
}
